{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:react/recommended",
      "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {
      "quotes": 0,
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".tsx",
            ".scss",
            ".mp3"
          ]
        }
      ],
      "semi": 0,
      "no-use-before-define": "off",
      "no-trailing-spaces": "warn",
      "@typescript-eslint/no-use-before-define": "off",
      "react/prop-types": "off",
      "camelcase": "off",
      "comma-dangle": "off",
      "react/display-name": [
        2
      ]
    }
  }
  